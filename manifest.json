{
    "manifest_version": 2,
    "name": "LinkedIn AdBlocker",
    "description": "Blocks your ads.",
    "version": "1.0",
    "author": "kzeen",
    "browser_action": {
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}